{
	"name": "gcn_grammar",

	"blocks": {
		"input": ["Input", "shape"],
		"GraphConv": ["GraphConv", "units", "activation"],
		"GraphSageConv": ["GraphSageConv", "units", "activation"],
		"GraphAttention": ["GraphAttention", "units", "activation"],
		"ARMAConv": ["ARMAConv", "units", "activation"],
		"APPNP": ["APPNP", "units", "activation"],
		"dropout": ["Dropout", "rate"]
	},

	"rules": {
		"<gcn>" : [["<conv>", "<layer_c>", "<dense>"]],
		"<layer_c>": [
			["<conv>"],
			["<dropout>"],
			["<layer_c>", "<layer_c>"]
		],
		"<dense>": [["<conv_type>", "<units>", "<dense_activation>"]],
		"<conv>": [["<conv_type>", "<units>", "<activation>"]],
		"<conv_type>": [["GraphConv"], 
						["GraphSageConv"], 
						["GraphAttention"], 
						["ARMAConv"], 
						["APPNP"]],
		"<dropout>": [["dropout", "<rate>"]],
		"<units>": [[32], [64], [128], [256]],
		"<activation>": [["relu"], ["sigmoid"]],
		"<dense_activation>": [["softmax"]],
		"<rate>": [[0.25], [0.33], [0.5]]
	}
}
